		"homepagePoem": {
			"type": "GridTextBox",
			"content": "<h1 class=\"h-3\" style=\"color: var(--colors-light);\">ROSES ARE RED<br>VIOLETS ARE BLUE<br>THIS TEMPLATE AIN'T PRETTY<br>IT JUST HELPS US PASS<br>OUR QA COMMITTEE</h1>",
